<div class="jumbotron">
  <div class="container">
    <h1 class="">OpenActive API Dashboard</h1>
    <p>A dashboard to support OpenActive API users &amp; publishers.</p>
  </div>
</div>

<div class="container">
<table class="table table-hover">
  <thead>
  <tr>
    <th scope="col">Provider</th><th scope="col">Endpoint</th><th scope="col">Status</th>
  </tr>
  </thead>
  <tbody>
  <% for dataset in datasets %>
    <tr>
      <td><a href="https://<%= dataset["id"] %>" target="_blank"><%= dataset["title"] %></a></td>
      <td><a href="<%= dataset["data_url"] %>" target="_blank"><%= dataset["data_url"] %></a></td>
      <td><span class="green-light"></span>Available</td>
    </tr>
  <% end %>
</tbody>
</table>

<small><a href="#wrapper">[top]</a></small>
</div>